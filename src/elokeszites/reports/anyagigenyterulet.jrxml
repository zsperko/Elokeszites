	<queryString>
		<![CDATA[SELECT 
  temp_anyagigeny.cikkszam,
  sum(temp_anyagigeny.mennyiseg) AS mennyiseg,
  keszlet.mennyiseg AS uzemi,
  temp_anyagigeny.keszlet
FROM
  temp_anyagigeny
  INNER JOIN terulet ON (temp_anyagigeny.terulet = terulet.azon)
  LEFT OUTER JOIN keszlet ON (temp_anyagigeny.cikkszam = keszlet.cikkszam)
  AND (temp_anyagigeny.raktar = keszlet.raktar)
GROUP BY
  temp_anyagigeny.cikkszam,
  keszlet.mennyiseg,
  temp_anyagigeny.keszlet
ORDER BY
  temp_anyagigeny.cikkszam]]>
