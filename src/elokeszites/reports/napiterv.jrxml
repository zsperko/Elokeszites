	<queryString>
		<![CDATA[SELECT 
  gr_muveletterv.grszam,
  muhelyek.szam,
  gyartasirend.cikkszam,
  gyartasirend.mennyiseg,
  gyartasirend.maradek,
  temp_napiterv.de,
  temp_napiterv.du,
  temp_napiterv.ej
FROM
  temp_napiterv
  INNER JOIN gr_muveletterv ON (temp_napiterv.grmuvazon = gr_muveletterv.azon)
  INNER JOIN muhelyek ON (temp_napiterv.muhelyazon = muhelyek.azon)
  INNER JOIN gyartasirend ON (gr_muveletterv.grszam = gyartasirend.rendszam)
ORDER BY
  muhelyek.sorrend,
  gyartasirend.cikkszam]]>
