		<![CDATA[SELECT 
  beerkezesek.datum,
  beerkezesek.ido,
  beerkezesek.szallitoszam,
  beerkezesek.biletaszam,
  beerkezesek.icon,
  szallitoszamok.szallitoszam AS oldszallito,
  eredet.megnevezes,
  kerulet.keruletnev,
  keszlet.mennyiseg
FROM
  beerkezesek
  LEFT OUTER JOIN keszlet ON (beerkezesek.biletaszam = keszlet.biletaszam)
  LEFT OUTER JOIN szallitoszamok ON (keszlet.sorszam = szallitoszamok.ronksorszam)
  INNER JOIN eredet ON (keszlet.eredet = eredet.eredetkod)
  INNER JOIN kerulet ON (keszlet.kerulet = kerulet.azon)
ORDER BY
  beerkezesek.szallitoszam,
  beerkezesek.biletaszam]]>
