	<queryString>
		<![CDATA[SELECT 
  visszadando.cikkszam,
  visszadando.mennyiseg,	
  visszadando.keszlet,
  visszadando.anyagigeny
FROM
  visszadando
  INNER JOIN cikktorzs ON (visszadando.cikkszam = cikktorzs.cikkszam)
  INNER JOIN anyagtipus ON (cikktorzs.anyagtipus = anyagtipus.megnevezes)
  INNER JOIN anyagigenybeallitas ON (anyagtipus.azon = anyagigenybeallitas.anyagtipus)
WHERE
  mennyiseg > 0 and anyagigenybeallitas.anyagigeny = 1
ORDER BY
  cikkszam]]>
	</queryString>
