<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="teljesitmenyek" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<parameter name="datum" class="java.lang.String"/>
	<parameter name="user" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT dszam,nev,muszak,terv,teny FROM
(
SELECT teljesitmenyek.dszam,dtorzs.nev,seged.megnevezes as muszak,
(sum((teljesitmenyek.tervmennyiseg * gr_muveletterv.szemelyido)) / 480) AS terv,
(sum((teljesitmenyek.mennyiseg * gr_muveletterv.szemelyido)) / 480) AS teny
FROM teljesitmenyek INNER JOIN dtorzs ON (teljesitmenyek.dszam = dtorzs.dszam)
INNER JOIN gr_muveletterv ON (teljesitmenyek.grszam = gr_muveletterv.grszam) AND
(teljesitmenyek.muvszam = gr_muveletterv.muveletkapcsolo) INNER JOIN
(SELECT napitervfejlec.dszam,muszakok.megnevezes FROM napitervfejlec
INNER JOIN muszakok ON (napitervfejlec.muszak = muszakok.azon)
WHERE napitervfejlec.datum = $P{datum} GROUP BY
napitervfejlec.dszam,muszakok.megnevezes UNION ALL
SELECT napiterv.dszam,muszakok.megnevezes FROM napiterv
INNER JOIN muszakok ON (napiterv.muszak = muszakok.azon)
WHERE napiterv.datum = $P{datum} GROUP BY
napiterv.dszam,muszakok.megnevezes)
seged ON (teljesitmenyek.dszam = seged.dszam) WHERE
teljesitmenyek.datum = $P{datum}
GROUP BY
teljesitmenyek.dszam,dtorzs.nev,seged.megnevezes ORDER BY
seged.megnevezes,dtorzs.nev) seged
WHERE terv <> 0]]>
	</queryString>
