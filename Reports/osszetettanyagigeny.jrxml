	<queryString>
		<![CDATA[SELECT 
  temp_anyagigeny.cikkszam,
  sum(temp_anyagigeny.mennyiseg) AS igeny,
  temp_anyagigeny.muhelyszam,
  terulet.megnevezes,
  temp_anyagigeny.keszlet,
  temp_anyagigeny.uzemikeszlet,
  seged.osszigeny
FROM
  temp_anyagigeny
  INNER JOIN terulet ON (temp_anyagigeny.terulet = terulet.azon)
  INNER JOIN cikktorzs ON (temp_anyagigeny.cikkszam = cikktorzs.cikkszam)
  INNER JOIN muhelyek ON (temp_anyagigeny.muhelyszam = muhelyek.szam)
  INNER JOIN (SELECT 
  temp_anyagigeny.cikkszam,
  sum(temp_anyagigeny.mennyiseg) AS osszigeny
FROM
  temp_anyagigeny
GROUP BY
  temp_anyagigeny.cikkszam) seged ON (temp_anyagigeny.cikkszam = seged.cikkszam)
GROUP BY
  temp_anyagigeny.cikkszam,
  temp_anyagigeny.muhelyszam,
  terulet.megnevezes,
  temp_anyagigeny.keszlet,
  temp_anyagigeny.uzemikeszlet,
  seged.osszigeny
ORDER BY
  cikktorzs.anyagtipus,
  muhelyek.sorrend,
  temp_anyagigeny.cikkszam]]>
	</queryString>
